# name: test/sql/large/deadlock_test_different_detectors.test
# description: First query uses detectors 1-3, second uses 4-6
# group: [large]

require h5db

# First query: detectors 1-3
query I
SELECT COUNT(*) FROM (
  SELECT * FROM h5_read('test/data/large/large_multithreading.h5',
      '/detector_1/time_offset', '/detector_1/event_id')
  UNION ALL
  SELECT * FROM h5_read('test/data/large/large_multithreading.h5',
      '/detector_2/time_offset', '/detector_2/event_id')
  UNION ALL
  SELECT * FROM h5_read('test/data/large/large_multithreading.h5',
      '/detector_3/time_offset', '/detector_3/event_id')
);
----
6000000

# Second query: DIFFERENT detectors 4-6
query I
SELECT COUNT(*) FROM (
  SELECT * FROM h5_read('test/data/large/large_multithreading.h5',
      '/detector_4/time_offset', '/detector_4/event_id')
  UNION ALL
  SELECT * FROM h5_read('test/data/large/large_multithreading.h5',
      '/detector_5/time_offset', '/detector_5/event_id')
  UNION ALL
  SELECT * FROM h5_read('test/data/large/large_multithreading.h5',
      '/detector_6/time_offset', '/detector_6/event_id')
);
----
6000000
