# name: test/sql/large/deadlock_test_3det_2times.test
# description: Test 3-detector query repeated 2 times
# group: [large]

require h5db

# 3 detectors - First time
query I
SELECT COUNT(*) FROM (
  SELECT * FROM h5_read('test/data/large/large_multithreading.h5',
      '/detector_1/time_offset',
      '/detector_1/event_id')
  UNION ALL
  SELECT * FROM h5_read('test/data/large/large_multithreading.h5',
      '/detector_2/time_offset',
      '/detector_2/event_id')
  UNION ALL
  SELECT * FROM h5_read('test/data/large/large_multithreading.h5',
      '/detector_3/time_offset',
      '/detector_3/event_id')
);
----
6000000

# 3 detectors - Second time
query I
