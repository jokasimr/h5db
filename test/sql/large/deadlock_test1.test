# name: test/sql/large/deadlock_test1.test
# description: Test exact first query from multithreading.test
# group: [large]

require h5db

# Exact first query - 2 detectors, 2 columns
query I
SELECT COUNT(*) FROM (
  SELECT * FROM h5_read('test/data/large/large_multithreading.h5',
      '/detector_1/time_offset',
      '/detector_1/event_id')
  UNION ALL
  SELECT * FROM h5_read('test/data/large/large_multithreading.h5',
      '/detector_2/time_offset',
      '/detector_2/event_id')
);
----
4000000
