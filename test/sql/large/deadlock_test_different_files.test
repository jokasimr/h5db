# name: test/sql/large/deadlock_test_different_files.test
# description: First query uses file A, second uses file B (copy)
# group: [large]

require h5db

# First query: original file
query I
SELECT COUNT(*) FROM (
  SELECT * FROM h5_read('test/data/large/large_multithreading.h5',
      '/detector_1/time_offset', '/detector_1/event_id')
  UNION ALL
  SELECT * FROM h5_read('test/data/large/large_multithreading.h5',
      '/detector_2/time_offset', '/detector_2/event_id')
  UNION ALL
  SELECT * FROM h5_read('test/data/large/large_multithreading.h5',
      '/detector_3/time_offset', '/detector_3/event_id')
);
----
6000000

# Second query: copy of file
query I
SELECT COUNT(*) FROM (
  SELECT * FROM h5_read('test/data/large/large_multithreading_copy.h5',
      '/detector_1/time_offset', '/detector_1/event_id')
  UNION ALL
  SELECT * FROM h5_read('test/data/large/large_multithreading_copy.h5',
      '/detector_2/time_offset', '/detector_2/event_id')
  UNION ALL
  SELECT * FROM h5_read('test/data/large/large_multithreading_copy.h5',
      '/detector_3/time_offset', '/detector_3/event_id')
);
----
6000000
