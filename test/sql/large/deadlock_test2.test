# name: test/sql/large/deadlock_test2.test
# description: Test first TWO queries from multithreading.test
# group: [large]

require h5db

# First query
query I
SELECT COUNT(*) FROM (
  SELECT * FROM h5_read('test/data/large/large_multithreading.h5',
      '/detector_1/time_offset',
      '/detector_1/event_id')
  UNION ALL
  SELECT * FROM h5_read('test/data/large/large_multithreading.h5',
      '/detector_2/time_offset',
      '/detector_2/event_id')
);
----
4000000

# Second query
query II
SELECT MIN(time_offset), MAX(event_id) FROM (
  SELECT * FROM h5_read('test/data/large/large_multithreading.h5',
      '/detector_1/time_offset',
      '/detector_1/event_id')
  UNION ALL
  SELECT * FROM h5_read('test/data/large/large_multithreading.h5',
      '/detector_2/time_offset',
      '/detector_2/event_id')
);
----
100	3999999
