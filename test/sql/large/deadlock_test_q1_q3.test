# name: test/sql/large/deadlock_test_q1_q3.test
# description: Test query 1 then query 3 (skip query 2)
# group: [large]

require h5db

# Query 1: 2 detectors
query I
SELECT COUNT(*) FROM (
  SELECT * FROM h5_read('test/data/large/large_multithreading.h5',
      '/detector_1/time_offset',
      '/detector_1/event_id')
  UNION ALL
  SELECT * FROM h5_read('test/data/large/large_multithreading.h5',
      '/detector_2/time_offset',
      '/detector_2/event_id')
);
----
4000000

